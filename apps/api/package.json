{
  "name": "@saas-template/api",
  "version": "1.0.0",
  "description": "SaaS Template API - NestJS Backend",
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "start": "node dist/main",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "biome check .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "migration:run": "cd ../../packages/database && pnpm typeorm migration:run -d src/config/data-source.ts",
    "migration:revert": "cd ../../packages/database && pnpm typeorm migration:revert -d src/config/data-source.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.10",
    "@nestjs/core": "^10.3.10",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/throttler": "^5.2.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "@saas-template/core": "workspace:*",
    "@saas-template/database": "workspace:*",
    "typeorm": "^0.3.20",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "class-validator": "^0.14.1",
    "class-transformer": "^0.5.1",
    "argon2": "^0.40.3",
    "joi": "^17.13.3",
    "helmet": "^7.1.0",
    "@nestjs/cli": "^10.4.2",
    "typescript": "^5.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.1.2",
    "@nestjs/testing": "^10.3.10"
  }
}
